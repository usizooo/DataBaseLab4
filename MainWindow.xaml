<Window x:Class="DataBaseLab4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataBaseLab4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Frame x:Name="frame" Margin="5,10,5,10" NavigationUIVisibility="Hidden"/>
        <Button x:Name="nextTable" Content="Котики/сотрудники" Foreground="White" FontFamily="Cascadia Code SemiBold" Grid.Row="3" Margin="5,5,5,5" Click="nextTable_Click"/>
      
    </Grid>
</Window>
